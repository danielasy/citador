{
  "name": "citador",
  "version": "0.1.0",
  "description": "Cria citações em formato ABNT.",
  "repository": {
    "type": "git",
    "url": "https://github.com/danielasy/citador"
  },
  "bugs": {
    "url": "https://github.com/danielasy/citador/issues"
  },
  "keywords": [
    "citacao",
    "ABNT"
  ],
  "main": "index.js",
  "scripts": {
